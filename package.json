{
	"name": "ms-irfandi-betest",
	"version": "1.0.0",
	"description": "A Technical Test for Backend Developer (TypeScript, Express, MongoDB, Redis, Kafka)",
	"main": "dist/index.js",
	"author": "irfnd <irfandiabimanyu@gmail.com>",
	"license": "MIT",
	"packageManager": "pnpm@9.12.2",
	"scripts": {
		"prepare": "husky",
		"build": "pnpm prism:push && pnpm prisma:seed && rimraf dist && tsc",
		"start": "node dist/app/index.js",
		"dev": "nodemon app/index.ts",
		"lint": "eslint \"**/*.ts\" --fix",
		"format": "prettier --write \"**/*.{ts,js,mjs,cjs,json,md}\" && prisma format",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:e2e": "vitest run -c ./vitest.config.e2e.mjs",
		"prisma:push": "prisma generate && prisma db push",
		"prisma:seed": "prisma db seed"
	},
	"dependencies": {
		"@prisma/client": "^5.21.1",
		"@scalar/express-api-reference": "^0.4.165",
		"@upstash/redis": "^1.34.3",
		"argon2": "^0.41.1",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^5.0.1",
		"helmet": "^8.0.0",
		"http-status-codes": "^2.3.0",
		"jsonwebtoken": "^9.0.2",
		"kafkajs": "^2.2.4",
		"morgan": "^1.10.0",
		"passport": "^0.7.0",
		"passport-jwt": "^4.0.1",
		"reflect-metadata": "^0.2.2",
		"tsyringe": "^4.8.0",
		"validator": "^13.12.0",
		"zod": "^3.23.8",
		"zod-validation-error": "^3.4.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@faker-js/faker": "^9.1.0",
		"@irfnd/prettier-config": "^1.0.0",
		"@types/compression": "^1.7.5",
		"@types/cors": "^2.8.17",
		"@types/express": "^5.0.0",
		"@types/jsonwebtoken": "^9.0.7",
		"@types/morgan": "^1.9.9",
		"@types/passport": "^1.0.17",
		"@types/passport-jwt": "^4.0.1",
		"@types/supertest": "^6.0.2",
		"@types/validator": "^13.12.2",
		"@vitest/coverage-v8": "^2.1.4",
		"eslint": "^9.13.0",
		"globals": "^15.11.0",
		"husky": "^9.1.6",
		"lint-staged": "^15.2.10",
		"nodemon": "^3.1.7",
		"prettier": "^3.3.3",
		"prisma": "^5.21.1",
		"rimraf": "^6.0.1",
		"supertest": "^7.0.0",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.12.2",
		"vite-tsconfig-paths": "^5.0.1",
		"vitest": "^2.1.4",
		"vitest-mock-extended": "^2.0.2"
	},
	"prettier": "@irfnd/prettier-config",
	"prisma": {
		"seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
	},
	"lint-staged": {
		"**/*.ts": "eslint --fix",
		"**/*.{ts,js,mjs,cjs,json,md}": "prettier --write"
	}
}
